Reset1C proc

        cli

        xor bx, bx
        mov es, bx
        mov bx, 4 * 1Ch                                   ; 8 interruption - keyboard
        
        mov es:[bx], offset Old1C                       ; change on new version

        mov ax, cs
        mov es:[bx+2], ax                                 ; NewSegment

        sti

        ret
        endp